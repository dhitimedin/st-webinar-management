{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "st-webinar-management/webinar",
	"title": "Webinar",
	"icon": "dashicons-megaphone",
	"category": "common",
	"keywords": ["webinar", "presentation", "event"],
	"textdomain": "st-webinar-management",
	"attributes": {
		"webinarId": {
		 	"type": "number"
		},
		"title": {
			"type": "string"
		},
		"subtitle": {
			"type": "string"
		},
		"thumbnail": {
			"type": "object"
		},
		"startDate": {
			"type": "string"
		},
		"endDate": {
			"type": "string"
		},
		"duration": {
			"type": "string"
		},
		"description": {
			"type": "array"
		},
		"registrationForm": {
			"type": "string"
		},
		"streamingLink": {
			"type": "string"
		},
		"highlights": {
			"type": "array",
			"default": []
		},
		"speakers": {
			"type": "array",
			"default": []
		},
		"isStartDatePickerOpen": {
			"type": "boolean",
			"default": false
		},
		"isEndDatePickerOpen": {
			"type": "boolean",
			"default": false
		}
	},
    "editorScript": "./assets/js/webinar.min.js",
	"editorStyle": "./assets/css/webinar.min.css",
    "supports": {
		"anchor": true
   	}
}
